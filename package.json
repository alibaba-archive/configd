{
  "name": "configd",
  "version": "0.0.2",
  "description": "Load your configs, merge into one json file",
  "main": "./lib/configd.js",
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "keywords": [
    "config",
    "json"
  ],
  "bin": {
    "configd": "./bin/configd.js"
  },
  "scripts": {
    "test": "[ -f test/config.coffee ] || cp test/config.example.coffee test/config.coffee && rm -rf test/dest && NODE_ENV=test ./node_modules/.bin/mocha --require should --require coffee-script/register --reporter spec test/main.coffee",
    "prepublish": "rm -rf lib && coffee -o lib -c src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teambition/configd.git"
  },
  "author": "Teambition",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teambition/configd/issues"
  },
  "homepage": "https://github.com/teambition/configd",
  "devDependencies": {
    "coffee-script": "^1.9.2",
    "express": "^4.12.3",
    "mocha": "^2.2.4",
    "should": "^6.0.1"
  },
  "dependencies": {
    "bluebird": "^2.9.24",
    "chalk": "^1.0.0",
    "commander": "^2.8.0",
    "lodash": "^3.7.0",
    "request": "^2.55.0"
  }
}
